<%= form_for([:admin, @category]) do |f| %>
    <%= render 'shared/error_messages', object: f.object %>

    <%= f.label :title %>
    <%= f.text_field :title %>

    <%= f.label :description %>
    <%= f.text_area :description %>

    <%= f.submit yield(:button_text), class: "btn btn-primary" %>
<% end %>
