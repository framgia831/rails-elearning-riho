<%= form_for([:admin, @category, @word]) do |f| %>
    <%= render 'shared/error_messages', object: f.object %>
	<div class="row">
		<div class="col-md-3 col-md-offset-3">
		    <%= f.label :word %>
		    <%= f.text_field :content %>
			<%= f.submit "Add Words", class: "btn btn-primary" %>
		</div>
	</div>
<% end%>

